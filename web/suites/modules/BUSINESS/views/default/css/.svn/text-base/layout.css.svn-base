/*
* @description: 产品分类列表页顶部焦点图
* @author: 	    Mouse
* @powered:     NINE-LEAF 
* @date:        2013-12-31
*/
.flex-control-nav {width: 100%; position: absolute; top: 500px; left:240px;  text-align: center; }
.flex-control-nav li {margin: 0 6px; display: inline-block; zoom: 1; *display: inline;}
.flex-control-paging li a {width: 15px; height: 15px; display: block; background: #666; background: rgba(151,138,134,1); cursor: pointer; text-indent: -9999px; -webkit-border-radius: 20px; -moz-border-radius: 20px; -o-border-radius: 20px; border-radius: 20px; -webkit-box-shadow: inset 0 0 3px rgba(0,0,0,0.3); -moz-box-shadow: inset 0 0 3px rgba(126,0,1,1); -o-box-shadow: inset 0 0 3px rgba(0,0,0,0.3); box-shadow: inset 0 0 3px rgba(126,0,1); }
.flex-control-paging li a:hover { background: #978a86; background: rgba(151,138,134,1); }
.flex-control-paging li a.flex-active { background: #7e0001; background: rgba(126,0,1,1); cursor: default; }
.flex-control-thumbs {margin: 5px 0 0; position: static; overflow: hidden;}
.flex-control-thumbs li {width: 25%; float: left; margin: 0;}
.flex-control-thumbs img {width: 100%; display: block; opacity: .7; cursor: pointer;}
.flex-control-thumbs img:hover {opacity: 1;}
.flex-control-thumbs .flex-active {opacity: 1; cursor: default;}
/*css end*/

