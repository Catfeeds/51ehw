/*框架*/
.container { position: relative; min-height: 100%; height: auto !important; height: 100%; background-color: white; }
.header { position: fixed; z-index: 999; width: 100%; background:#1A1A1A; text-align: center; vertical-align: middle; color: #3f3f3f; overflow: hidden; white-space: nowrap; text-overflow: ellipsis; }
.header a {z-index: 999;}
.header .title { position: absolute;font-size: 18px; line-height: 40px; color: #fff; font-weight: bold; padding: 5px; top: 0; left: 0; right: 0; text-align: center;}
.header .title b{ float:right;margin-left: -26px;margin-top: -3px;}
.header [class*="icon-"], .btn-header { color: white; font-size: 20px; padding: 0 12px; height: 44px; line-height: 50px; text-align: center; float: left; position: relative; }
.icon-cartfill span { color: white; font-size: 12px; background-color: red; border-radius: 12px; padding: 3px; vertical-align: top; }
.page { margin: 0 auto;  padding-top: 50px; padding-bottom: 45px; max-width: 640px;}
.footer { position: fixed; bottom: 49px; z-index: 999; clear: both; width: 100%; height: 44px; background: #444444; margin: 0 !important }
/*取消顶部固定浮动*/
.header_fix { width: 100%; background: #fea33b; text-align: center; vertical-align: middle; color: #3f3f3f; overflow: hidden; white-space: nowrap; text-overflow: ellipsis; }
.header_fix .title { margin: 0 100px; font-size: 18px; line-height: 34px; color: #fff; font-weight: bold; padding: 5px; text-align: center; overflow: hidden; white-space: nowrap; text-overflow: ellipsis; }
.header_fix [class*="icon-"], .btn-header { color: white; font-size: 20px; padding: 0 12px; height: 44px; line-height: 44px; text-align: center; float: left; position: relative; }
.page_fix { margin: 0 auto; max-width: 640px; padding-bottom: 44px; }
.nav-tab li .button { padding: 0; width: 100%; background: #3e3e3e; font-weight: normal; line-height: 44px; color: #fff; }
.nav-tab > li { float: left; }
.nav-tab .button01 { display: block;text-align: center;line-height:20px;padding-top: 4px; width: 100%; background: #3e3e3e; font-weight: normal; color: #fff;font-size: 15px;}
/*margin: 0 0 0 -1px; */

.nav-tab > *:first-child { margin-left: 0; }
.nav-tab li:nth-child(1) { width: 50%; line-height: 0; }
.nav-tab li:nth-child(2) { width: 50%; line-height: 0; }
.nav-tab li:nth-child(3) { width: 50%; line-height: 0; }
.nav-tab li:nth-child(4) { width: 50%; line-height: 0; }
.button { position: relative; display: inline-block; margin: 0; outline: 0; background-color: #f7f7f8; color: #717171; text-align: center; text-decoration: none; font-weight: bold; font-size: 16px; font-family: inherit; line-height: 1; cursor: pointer; }
.bg-red { background-color: #fe4101!important; color: #fff!important; }
.bg-color {background-color: #1A1A1A!important; color: #fff!important;}
.content { padding: 20px; }
.shoucang {color: #fff;display: block;font-size: 18px;text-align: center;}
.gouwuche {color: #fff;display: block;font-size: 18px;text-align: center;}
/*page 导航*/
.menu { border-bottom: 1px solid #cccccc;border-top: 1px solid #cccccc; }
.menu li { float: left; width: 50%; border-right: 1px solid #cccccc; }
.menu li:last-of-type { border: 0; }
.menu a { width: 100%; background-color: white; float: left; padding: 1em 0; text-align: center; font-size: 1em; }
.menu em { font-size: 20px; }
.menu strong { display: block; }
.menu a { width: 100%; background-color: white; float: left; padding: 10px 0; text-align: center; font-size: 14px; }
/*广告图片*/
.swiper-container { /*    height: 400px;*/ margin-bottom: 20px; }
.swiper-slide { text-align: center; font-size: 18px; background: #e1e1e1; display: -webkit-box; display: -ms-flexbox; display: -webkit-flex; display: flex; -webkit-box-pack: center; -ms-flex-pack: center; -webkit-justify-content: center; justify-content: center; -webkit-box-align: center; -ms-flex-align: center; -webkit-align-items: center; align-items: center; }
/*品牌广告列表*/
.brand-list { background-color: #eeeeee; padding: 10px; }
.brand-item span{ float:left; margin-left:5px;}
.brand-item span a{ color:#ff9000;}
.brand-item { background-color: white; text-align: center; margin-bottom: 10px; }
.brand-item a { font-weight: bold; }
.brand-item p { padding: 20px 0; }
/*底图登录、信息公司*/
.login { font-size: 12px; }
.login a { padding: 0 5px; font-size: 12px; }
.login div { padding: 20px 10px; overflow:hidden;}
.login .name { text-decoration: underline; padding-right: 1em; }
.login address { color: #666666; font-style: normal; border-top: 1px solid #cccccc; background-color: #eeeeee; line-height: 1.6em; padding: 18px 0; font-size: 12px; }
.login address a { color: #666666; }
.c { text-align: center; }
/*商品列表页、商品筛选*/
.goods-nav { width: 100%; height: 34px; border-bottom: 1px solid #e6e6e6; display: table; cursor: pointer; }
.nav-item { display: table-cell; text-align: center; padding: 0 12px; overflow: hidden; height: 33px; line-height: 33px; font-size: 14px; color: #999; }
.nav-item.focus { color: #000; border-bottom: 2px solid #000; }
.sort { width: 15px; line-height: normal; overflow: hidden; display: inline-block; vertical-align: middle; }
.nav-item .sort i { position: relative; top: -3px; font-size: 14px; -webkit-transform: scale(0.83); -moz-transform: scale(0.83); -ms-transform: scale(0.83); -o-transform: scale(0.83); transform: scale(0.83); margin-left: 3px; display: inline-block; font-weight: bold; }
.nav-item.focus .sort i.cur { color: #444; }
.nav-item.focus .sort i { color: #c0c0c0; }
.nav-item .sort .up { top: 3px; }
.nav-item .sort .down { top: -6px; }
/*商品列表*/
.goods-container { position: relative; min-height: 380px; padding-left: 10px; }
.goods-info { float: left; width: 50%; margin: 10px 0; position: relative; text-align: center; }
.goods-info .title { margin-top: 10px; overflow: hidden; white-space: nowrap; text-overflow: ellipsis; height: 16px; line-height: 16px; font-size: 1em; text-align: left; font-weight: bold; }
