.ui-shop-cart { margin: 20px 0; }
.ui-cart-bd { border: 1px solid #e5e3e4; }
.ui-cart-goods { overflow: hidden; }
.ui-cart-goods dt {
    padding: 0 20px 0 150px; background: #fafafa; border-bottom: 1px solid #ededed; color: #8f8c89;
    font-size: 14px; font-weight: bold;
}
.ui-cart-goods .col { float: left; height: 40px; line-height: 40px; padding-right: 10px; font-size: 14px; }
.ui-cart-goods .col1 { width: 400px }
.ui-cart-goods .col2 { width: 70px; text-align: center }
.ui-cart-goods .col3 { width: 150px; text-align: center }
.ui-cart-goods .col4 { float: right; width: 60px; text-align: right; padding-right: 0 }
.ui-cart-goods .item { padding: 20px 0; border-bottom: 1px solid #ededed }
.ui-cart-goods .ui-goods-pic { float: left; width: 150px; text-align: center }
.ui-cart-goods .ui-goods-pic img { width: 90px; height: 120px }
.ui-cart-goods .goods-info { float: left; width: 810px }
.ui-cart-goods .goods-info .goods-count { border-bottom: 1px solid #ededed }
.ui-cart-goods .goods-info .goods-count select { vertical-align: text-top }
.ui-cart-goods .goods-info .goods-count .col1 { overflow: hidden; text-overflow: ellipsis; white-space: nowrap }
.ui-cart-goods .goods-info .goods-count .col1 a { color: #434a54; font-size: 18px; font-weight: bold; }
.ui-cart-goods .goods-info .goods-count .col1 a:hover { color: #c20000 }

.ui-cart-goods .goods-info .goods-count .col4 { color: #ff6f3d }
.ui-cart-goods .goods-info .goods-count .pre-info {
    float: right; padding-right: 0; color: #ed5567; font-size: 12px;
}

.ui-cart-goods .goods-info ul {
    padding: 5px 0 0; line-height: 2; color: #8f8c89; font-size: 12px;
}
a.close {
    background: url(../images/remove.png) no-repeat 4px 4px; display: inline-block;
    width: 18px;
    height: 18px;
}
a.close:hover {
    background: #c20000 url(../images/remove_2.png) no-repeat 4px 4px; }
.ui-goods-num {
    width: 50px; height: 18px; font-size: 12px; vertical-align: middle;
    line-height: 18px; padding: 0; border: 1px solid #E6E9ED; -webkit-border-radius: 5px;
    -moz-border-radius: 5px; border-radius: 5px; text-align: center; color: #434A54;
}
.icon-common {
    display: block;
    background-image: url(../images/icon-common.png);
    background-repeat: no-repeat; cursor: pointer;
}
.change-goods-num a {
    display: inline-block; width: 20px; height: 20px; margin: 0 5px;
    background: #CCD1D9; -webkit-border-radius: 5px;
    -moz-border-radius: 5px; border-radius: 5px; vertical-align: middle;
    opacity: .7; filter: alpha(opacity=70);
}
.icon-common-add { width: 20px; height: 20px; background-position: -19px 5px; }
.icon-common-negative { width: 20px; height: 20px; background-position: 5px 5px; }

.ui-cart-count {
    position: relative;
    margin: 20px;
    padding: 20px;
    background-color: #FAFAFA;
}

.ui-cart-price { font-size: 14px; }
.ui-cart-price span { color: #fd713e; font-weight: bold; height: 40px; }
.ui-cart-price span strong {
    font-size: 30px; display: inline-block; _display: inline; zoom: 1; line-height: 30px;
}

.ui-cart-btn { padding: 0 20px 20px; text-align: right; }
.buy-dakelight { margin-left: 5px; }
.but {
    display: inline-block;
    margin-bottom: 0;
    font-size: 14px; font-weight: bold;
    height: 32px;
    line-height: 32px;
    text-align: center;
    cursor: pointer;
    background-color: #FFF;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    color: #656D78;
    padding: 0;
    width: 140px;
}
a.ui-shop-to {
    color: #fff;
    text-shadow: 1px 1px 1px #a88f83;
    background-color: #b4988a;
    background: -o-linear-gradient(top, #b4988a, #c0aa9d);
    background: -moz-linear-gradient(top, #b4988a, #c0aa9d);
    background: -webkit-gradient(linear, left top, left bottom, from(#b4988a), to(#c0aa9d));
    filter: progid:DXImageTransform.Microsoft.Gradient(startcolorstr='#b4988a', endcolorstr='#c0aa9d');
    background: -ms-linear-gradient(top, #b4988a, #c0aa9d);
}
a.ui-shop-to:hover {
    color: #fff; text-decoration: none;
    background-color: #c0aa9d;
}
a.ui-shop-checkout {
    color: #fff;
    text-shadow: 1px 1px 1px #8a001b;
    background-color: #9b001f;
    background: -o-linear-gradient(top, #a40928, #9b001f);
    background: -moz-linear-gradient(top, #a40928, #9b001f);
    background: -webkit-gradient(linear, left top, left bottom, from(#a40928), to(#9b001f));
    filter: progid:DXImageTransform.Microsoft.Gradient(startcolorstr='#a40928', endcolorstr='#9b001f');
    background: -ms-linear-gradient(top, #a40928, #9b001f);
}
a.ui-shop-checkout:hover { background-color: #93001d; color: #fff; text-decoration: none; }

.ui-cart-info {
    padding: 15px 20px; background: #fefbea; color: #974c1b; font-size: 16px;
    border: 1px solid #efe09f; border-radius: 5px 5px 0 0;
}
.ui-cart-info li { padding: 5px 0; }
.ui-cart-info .ui-pay-money { color: #ff6f3d }
.ui-cart-info p { font-size: 14px; line-height: 24px; }
.ui-cart-info p { padding-left: 54px; margin-top: -19px }
.ui-cart-info p .line { margin: 0 10px }
.ui-pay-system-box { border: 1px solid #EDEDED; border-top: none; }
.ui-system-title {
    height: 40px; line-height: 40px; padding-right: 10px; background: #FAFAFA;
    border-bottom: 1px solid #EDEDED; border-top: 1px solid #EDEDED;
}
.ui-system-title h3 { color: #8F8C89; font-size: 14px; font-weight: bold; padding-left: 20px; }
.ui-system-bd .ui-system-payment-list { margin-left: 20px; padding: 20px; background: #FFF; }
/*.ui-system-bd .ui-system-payment-list li { width: 167px; padding: 10px 0; float: left; }*/
.ui-system-bd .ui-system-payment-list li { width: 206px; padding: 10px 0; float: left; }
.ui-system-bd .ui-system-payment-list li img{border:1px solid #d2d2d2; vertical-align:middle; margin-left:3px;}
.ui-system-bd .ui-system-payment-list li label { cursor: pointer; }
.ui-system-bd .ui-system-payment-list li input, .ui-system-bd .ui-system-payment-list li img { vertical-align: middle; }

.ui-pay-success {
    width: 880px; margin: 20px auto; padding: 40px 20px;
    background-color: #f4ffef;
    border: 1px dotted #bdbdbd;
}

.ui-pay-success ul { padding-left: 275px; }
.ui-success-title { height: 40px; padding-bottom: 5px; padding-left: 20px; position: relative; }

.ui-success-title i {
    width: 42px; height: 42px; background: url(../images/ico_pa.png) no-repeat;
    float: left; margin-right: 15px; display: inline; position: absolute; left: -45px; top: -5px;
}
.ui-success-title strong { float: left; color: #429800; font-size: 14px; line-height: 40px; }
.ui-success-list { padding-left: 20px; height: 24px; line-height: 24px; font-size: 12px; color: #666 }
.ui-success-list label { margin-right: 10px; }
.ui-success-list strong { color: red; font-size: 14px; }
.ui-success-but{ padding-left: 15px;margin-top: 20px;}


