/**/
.ui-crumb {
    width:980px; margin:10px auto 0; color:#666;
    border-bottom: 1px dotted #9f9f9f; height: 25px; line-height: 25px;
}
.ui-crumb a { color:#666; }
.ui-crumb a:hover { color:#666; text-decoration:underline; }
.ui-crumb a.on { color:#666; font-weight:bold; }
.ui-crumb span{ color: #999;}



/*ui-history*/
.ui-history{ overflow: hidden; border: 1px solid #dcdcdc;}
.ui-history-title{ height: 35px; line-height: 35px; background: #f7f7f7; border-bottom:1px solid #dcdcdc; }
.ui-history-title h3{ font-weight: bold; font-size: 14px; padding-left: 20px;}
.ui-history-bd{  position: relative;}/*padding:15px 10px;*/
.ui-history-bd .next,.ui-history-bd .pre{
    width: 30px; height: 60px; display:block; cursor: pointer;
    position: absolute; background: url(../images/next_bj.gif) no-repeat;
}
.ui-history-bd .next{right:5px;top:50%; margin-top: -60px; background-position: 0 -60px;}
.ui-history-bd .pre{left:5px;top:50%; margin-top: -60px;}
.ui-history-list{ width: 177px; float:left; margin:0 28px;}
.ui-history-list .list-pic{
    width: 175px; height: 210px; border: 1px solid #ccc;
}
.list-bgline{height: auto;
    overflow: hidden;
    /*padding: 4px 0 0 12px;*/
	padding: 4px 0 0 6px;
    width: 999999px;}
.ui-history-list .list-info{margin:5px 0;}
.ui-history-list .list-price{color: #C53F4C;font-weight: bold;    font-size: 14px;}
.list-but{ margin: 5px 0;}
a.history-but{
    background: url(../images/but_bj.gif) no-repeat;
    width: 72px; height: 22px; text-align: center; line-height: 22px;color: #666;
}
a.history-but:hover{ color: #333; text-decoration: none;}

/*
* @description: 分页
* @author: 	    Muke
* @powered:     NINE-LEAF 
* @date:        2014-01-28
*/
ul.page {text-align: center; }
ul.page li,ul.page li.current{
	display: inline-block;
    height: 28px;
    line-height: 28px;
    border: solid 1px #AA998A;
    font-size: 14px; color: #666;
    padding: 0 12px;
    background-color: #F7F5F2;
    margin: 0 3px;
	float:left;
}
ul.page li a{color: #666;}
ul.page li.prev{ background: #f1ece6 url(../images/cat_r.gif) no-repeat 10px 9px;padding: 0 10px 0 25px;}
ul.page li.next{background: #f1ece6 url(../images/cat_l.gif) no-repeat 12px 9px;padding: 0 10px 0 25px;}
ul.page li.current{color: #FFF;background: #9d0320;
    border: 1px solid #9d0320;}

.ui-page { text-align: center; }
.ui-page .prev, .ui-page .next {
    border: 1px solid #aa998a; display: inline-block; font-size: 14px;
    height: 28px; margin: 5px 10px 0 0; line-height: 28px; padding: 0 10px 0 25px;
}
.ui-page .prev { background: #f1ece6 url(../images/cat_r.gif) no-repeat 10px 9px; }
.ui-page .next { background: #f1ece6 url(../images/cat_l.gif) no-repeat 12px 9px; }
.ui-page a {
    display: inline-block;
    height: 28px;
    line-height: 28px;
    border: solid 1px #AA998A;
    font-size: 14px; color: #666;
    padding: 0 12px;
    background-color: #F7F5F2;
    margin: 0 3px;
} 
a.ui-paging-current, a.ui-paging-current:hover {
    color: #FFF;
    background: #9d0320;
    border: 1px solid #9d0320;
}




