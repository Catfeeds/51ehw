 .stored_zhong_ul_di a{ display:block;background:#fe4d52;border-radius:8px; height:40px; line-height:40px; margin:40px 20px; color:#ffffff; font-size:15px; text-align:center; font-weight:bold}
   
 /*分店管理*/
 .stored_value_div1{ overflow:hidden;width:100%; padding-bottom:10px;}
 .stored_value_div1 a{ height:50px; line-height:50px; padding:0 15px;display:block;  background:#fff;color:#333333; font-size:15px; border-bottom:1px solid #dddddd;}
 .stored_value_div1 span{ float:left}
 .stored_value_div1 i{color:#333333; line-height:50px; font-size:20px; float:right; font-weight:normal;-webkit-transform: rotate(180deg); display:block}
 .order_branch{ overflow:hidden; padding-bottom:60px;}
 .order_branch_top{ overflow:hidden; background:#fff;border-bottom: 1px solid #cccccc; padding:0 15px;} 
 .order_branch_top li{text-align: center;float: left;height: 45px; width:33.3%; height:43px; line-height:43px; font-size:15px;}
 .order_branch_top li a{ display:inline-block; color:#333333;}
 .order_branch_zhong{ overflow:hidden}
 .order_branch_zhong_ul li{ overflow:hidden;border-bottom:1px solid #dddddd; padding:15px 15px 5px 15px; background:#fff; font-size:14px;}
 .order_branch_zhong_ul li a{ display:block}
 .order_branch_divk{display: -webkit-box; display: -moz-box;display: -ms-flexbox; display: box; margin-bottom:10px;}
 .order_branch_di_left{overflow: hidden;box-sizing: border-box; box-flex: 1; -webkit-box-flex: 1; -moz-box-flex: 1; -ms-flex: 1;}
 .order_branch_di_left span{text-overflow: ellipsis;  overflow: hidden;  white-space: nowrap; display:block; width:100%;}
 .order_branch_zhong ul{ display:none;}
 .order_branch_zhong .action{ display:block}
 .order_branch_zhong_di{ position:fixed; bottom:0; width:100%; left:0; height:50px; line-height:50px; text-align: center; font-size:15px;border-top:1px solid #dddddd; background:#fff; }
 .order_branch_zhong_di a{color:#333333; display:block;}
.screening_time{ overflow:hidden; padding:50px 20px; font-size:15px;}
 .screening_time input{ border:none; width:40%; height:30px; position:relative; line-height:30px; text-align:center; overflow:hidden; font-size:15px; background:#fff; border-bottom:1px solid #dddddd; outline:none; -webkit-appearance:none; border-radius:0}
 .screening_time div{ margin:0 5%;display:inline-block; text-align:center; height:30px; line-height:30px;}
input[type="date"]:before{
    color:#A9A9A9;
    content:attr(placeholder);
	text-align:center;
	width:100%; position:absolute;
}
input[type="date"].full:before {
    color:black;
    content:""!important;
	text-align:center;
	width:100%; position:absolute;
}


/*遮罩层样式*/
.mask{z-index: 1000;position: fixed;top: 0px;left: 0px;width: 100%;height: 100%;background: rgba(0,0,0,.6);}
.mask .mask-content{ width: 90%; position: absolute; top: 50%; margin-left: 5%; margin-top: -43px; background: white; height: 86px; text-align: center;}
.mask .mask-content .del-p{color: #555;height: 45px;line-height:45px;font-size: 18px;border-bottom: 1px solid #D1D1D1;}
.mask-content .check-p{height: 40px;line-height: 40px;position: absolute;bottom:0px;left: 0;width: 100%;}
.mask-content .check-p span{width: 49%; float:left;text-align: center;color:#d4361d ;font-size: 18px;}
.check-p .del-com{border-right: 1px solid #D1D1D1;}
