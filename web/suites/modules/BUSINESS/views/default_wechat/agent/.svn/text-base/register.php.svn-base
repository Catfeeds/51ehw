<title>注册</title>
<link rel="stylesheet" type="text/css" href="fonts/iconfont.css">
<link rel="stylesheet" type="text/css" href="css/style.css">
</head>
<body>
<!--全局开始-->
<div id="warp"> 
  <!--头部开始-->
  <div class="header">
    <div class="tou">
      <div class="header_tou"><!--登录开始-->
        <h4><a href="#">51易货网合伙人管理平台</a></h4>
        <div class="header_tou_r">
          <div class="header_tou_r1"><a href="#">查询管理</a></div>
          <div class="header_tou_r2">
            <div class="header_n">
             <p>你好，<a href="#">请登录</a> </p>
             <p>---</p>
             </div>
            <small class="icon-touxiang"></small>
          </div>
        </div>
     </div>
     </div>
    <!--登录结束--> 
  </div>
  <!--头部结束--> 
  <!--注册帐号开始-->
  <div class="account">
      <div class="account_c">
        <div class="account_c_n">
             <h5>注册</h5>
             <div class="enter">
          <div class="rzhu">
            <div class="form_rzhu zhu1"> <span class="tiao1">用户名 :</span>
              <input type="text" class="biaod" id="placeholder" placeholder ="请输入用户ID">
            </div>
            <div class="form_rzhu zhu2"><span class="tiao1">Email地址 :</span>
               <input type="password" id="placeholder" name="nloginpwd" class="biaod" tabindex="2" autocomplete="off" placeholder="请输入登录密码">
            </div>
            <div class="form_rzhu zhu3"> <span class="tiao1">密码 :</span>
          <input type="password" id="placeholder" name="nloginpwd" class="biaod" tabindex="2" autocomplete="off" placeholder="">
            </div>
            <div class="form_rzhu zhu4"> <span class="tiao1">确认密码 :</span>
             <input type="password" id="placeholder" name="nloginpwd" class="biaod" tabindex="2" autocomplete="off" placeholder="">
            </div>
             <div class="form_rzhu zhu5"><span class="tiao1">企业名称 :</span>
              <input type="text" class="biaod" id="" placeholder ="">
            </div>
           <div class="form_rzhu zhu6"> <span class="tiao1">角色:</span>
           
              <select name="theme" id="theme" class="theme">
                <option selected="selected">城市子公司用户</option>
                <option value="">广东省</option>
                <option value="">四川省</option>
              </select></div>
            <div class="form_rzhu zhu6"> <span class="tiao1">区域:</span>    
              <select name="theme" id="theme" class="theme">
                <option selected="selected">全国</option>
                <option value="">广州市</option>
                <option value="">中山市</option>
                <option value="">珠海市</option>
                <option value="">湛江市</option>
              </select>
           </div>
            <div class="form_rzhu zhu7"><span class="tiao1">短信验证码:</span>
              <input type="text" class="biaod1" id="" placeholder ="">
              <span class="tiao2">获取手机验证码</span>
            </div>
            <div class="form_rzhu zhu8">
	                        <input type="checkbox" id="protocol" name="Preg_index_none_check" checked="checked">我已阅读并同意
							<a target="_blank" href="javascript:void(0)" name="Preg_index_none_hyzc" tabindex="-1">《51易货网注册协议》</a>
                        </div>
            <div class="form_rzhu zhu11"> <span class="tiao3"><a href="#">提交</a></span> </div>
          </div>
        </div>
        </div> 
      
      </div>
