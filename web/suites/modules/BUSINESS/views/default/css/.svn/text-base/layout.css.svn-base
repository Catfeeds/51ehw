#left_scroll{position: absolute;left:10px;top:100px;width:30px;height:60px;cursor: pointer;cursor: hand; 
background:url(../images/next_bj.gif) no-repeat 0 0;} 
#right_scroll{position: absolute;top:100px;right:10px; width:30px;height: 60px;cursor: pointer;cursor: hand; 
