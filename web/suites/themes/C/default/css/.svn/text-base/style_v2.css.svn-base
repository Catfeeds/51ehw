.eh_cart .eh_cart_list ul li .con{float: left;}
.eh_cart .eh_cart_list ul li .price{float: left;color: #fea33b; font-size: 14px;font-weight: 700;}
.eh_cart .eh_cart_list ul li .number{float: left; margin-left: 5px; color: #666;}
.eh_cart .eh_cart_list ul li .btn a{/*float: left;*/float:right; padding:2px 6px; background: #fea33b; color: #fff; font-size: 12px;}
.eh_cart .eh_cart_list .eh_balance {background: #fff1e1;=: 100%;padding: 8px 0 8px 20px;display: block;\: 20px;font-size: 14px;color: #666;}
.eh_cart .eh_cart_list .eh_balance p{float: left;height: 30px;line-height: 30px;}
.eh_cart .eh_cart_list .eh_balance a{background: #72c312;color: #fff;font-size: 16px;float: right;text-align: center;padding: 4px 30px;margin-right: 12px;}
.eh_head_btn { width: 95px; height: 35px; line-height: 35px; background: #fea33b; color: #fff;  text-align: center; font-size: 14px; -moz-border-radius: 3px; -ms-border-radius: 3px; -webkit-border-radius: 3px; -o-border-radius: 3px;display: block; }
.eh_head_btn:hover { color: #fff; }
.eh_head_btn > i { margin-right: 4px; }

/*** 顶站菜单、站点菜单 ***/
.sf-menu, .sf-menu * { margin: 0; padding: 0; list-style: none; }
.sf-menu li { position: relative; }
.sf-menu ul { position: absolute; display: none; top: 100%; left: -30px; z-index: 999999; }
.sf-menu > li { float: left; }
.sf-menu > li.line { color: #b2b2b2; }
.sf-menu > li i { float: left; margin-top: 11px; color: #fea33b; margin-right: 2px; }
.sf-menu > li a span { color: #fea33b; margin-left: 4px; }
.sf-menu li:hover > ul, .sf-menu li.sfHover > ul { display: block; }
.sf-menu a { display: block; position: relative; }
.sf-menu ul ul { top: 0; left: 100%; }
.sf-menu { float: left; }
.sf-menu ul { box-shadow: 2px 2px 6px rgba(0,0,0,.2); min-width: 12em; *width: 12em;
}
.sf-menu a { padding: 0 1em; text-decoration: none; zoom: 1;/* IE7 */ }
.sf-menu a { color: #666; }
.sf-menu li { white-space: nowrap; * white-space: normal;
-webkit-transition: background .2s; transition: background .2s; }
.sf-menu ul li { background: #fea33b; height: 30px; line-height: 30px; border-top: 1px solid #fea33b; }
.sf-menu ul li:first-child { border: 0; }
.sf-menu ul ul li { background: #fff; }
.sf-menu li a:hover, .sf-menu li.sfHover { -webkit-transition: none; transition: none; color: #fea33b; }
.sf-menu ul li a { color: #fff; background: #fea33b; }
.sf-menu ul li a:hover, .sf-menu ul li.sfHover { background: #fff; color: #fea33b; }
.sf-menu > li i.icon-coordinate { float: left; margin-top: 0px; color: #fea33b; margin-right: 2px; font-size: 20px; margin-left: 10px }
.sf-arrows .sf-with-ul { padding-right: 1em; }
.sf-arrows .sf-with-ul:after { content: ''; position: absolute; top: 50%; right: 1em; margin-top: -3px; height: 0; width: 0; border: 5px solid transparent; border-top-color: #dFeEFF; border-top-color: rgba(255,255,255,.5); }
.sf-arrows > li > .sf-with-ul:focus:after, .sf-arrows > li:hover > .sf-with-ul:after, .sf-arrows > .sfHover > .sf-with-ul:after { border-top-color: white; }
.sf-arrows ul .sf-with-ul:after { margin-top: -5px; margin-right: -3px; border-color: transparent; border-left-color: #dFeEFF; border-left-color: rgba(255,255,255,.5); }
.sf-arrows ul li > .sf-with-ul:focus:after, .sf-arrows ul li:hover > .sf-with-ul:after, .sf-arrows ul .sfHover > .sf-with-ul:after { border-left-color: white; }
